<h2>Show result</h2>

<ul>

  <li>
    <strong>Value:</strong>
    <%= @result.value %>
  </li>

  <li>
    <strong>Type:</strong>
    <%= @result.type %>
  </li>

  <li>
    <strong>Date:</strong>
    <%= @result.date %>
  </li>

  <li>
    <strong>Time:</strong>
    <%= @result.time %>
  </li>

  <li>
    <strong>Notes:</strong>
    <%= @result.notes %>
  </li>

</ul>

<%= link "Edit", to: result_path(@conn, :edit, @result) %>
<%= link "Back", to: result_path(@conn, :index) %>
